{
	"manifest_version": 2,
	"name": "Trello Scrum",
	"version": "1.0",
	"description": "Adds Scrum to your Trello",
	"content_scripts": [
		{
			"matches": ["https://trello.com/*"],
			"js": ["jquery-1.7.1.min.js","trelloscrum.js"],
			"css": ["trelloscrum.css"]
		}
	],
    "icons": { "128": "trello-scrum-icon.png" },
	"web_accessible_resources": [
		"images/block-icon.png",
		"images/storypoints-icon.png",
		"images/unit-icon.png",
		"images/value-icon.png"
	  ]
}
